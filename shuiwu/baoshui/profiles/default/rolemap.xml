<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="ShuiguanyuanMember" />                
  </roles>	
  <permissions>
    <!-- Map the "add" permissions for the various content types. Note that
         we let the "Contributor" role (in a folder) be allowed to add content
         when the "Owner" is, so that we can effectively delegate control
         over a folder. Some content types, such as project folders  are considered to be 
         so "structural" that only administrators
         can add them.
      -->
	<permission name="View" acquire="True">
    <role name="Owner" />		
    <role name="Contributor"/>
    <role name="Editor"/>
    <role name="Reader"/>
    <role name="Reviewer"/>
		<role name="Manager"/>
		<role name="Member"/>		
		<role name="Site Administrator"/>		
		<role name="ShuiguanyuanMember"/>		
    </permission>
    
	<permission name="Add portal content" acquire="True">
    <role name="Owner" />		
    <role name="Contributor"/>
    <role name="Editor"/>
    <role name="Reviewer"/>
		<role name="Manager"/>
		<role name="Member"/>		
		<role name="Site Administrator"/>		
		<role name="ShuiguanyuanMember"/>	
    </permission>
	<permission name="Modify portal content" acquire="True">
    <role name="Owner" />		
    <role name="Contributor"/>
    <role name="Editor"/>
    <role name="Reviewer"/>
		<role name="Manager"/>
		<role name="Member"/>		
		<role name="Site Administrator"/>		
		<role name="ShuiguanyuanMember"/>		
    </permission>    

	<permission name="Delete objects" acquire="True">
    <role name="Owner" />		
    <role name="Contributor"/>
		<role name="Manager"/>	
		<role name="Site Administrator"/>		
    </permission>        
	
    <permission name="Access contents information" acquire="True">
    <role name="Contributor"/>
    <role name="Editor"/>
    <role name="Reader"/>
    <role name="Reviewer"/>
		<role name="Manager"/>
		<role name="Member"/>		
		<role name="Site Administrator"/>		
		<role name="ShuiguanyuanMember"/>					  
    </permission>

	<permission name="List folder contents" acquire="True">
		<role name="Contributor"/>
		<role name="Editor"/>
		<role name="Manager"/>
		<role name="Owner"/>
		<role name="Reviewer"/>
		<role name="ShuiguanyuanMember"/>				
		<role name="Site Administrator"/>
	</permission>			
									
	<permission name="shuiwu.baoshui:Add nashuiku" acquire="True">
		<role name="Site Administrator"/>
		<role name="Manager"/>		
	</permission>
	
	<permission name="shuiwu.baoshui:Add nashuiren" acquire="True">
		<role name="Site Administrator"/>
		<role name="Manager"/>
		<role name="ShuiguanyuanMember"/>		
	</permission>
	
	<permission name="shuiwu.baoshui:Modify nashuiku" acquire="True">
		<role name="Site Administrator"/>
		<role name="Manager"/>		
		<role name="Editor"/>
	</permission>	
	<permission name="shuiwu.baoshui:Modify nashuiren" acquire="True">
		<role name="Site Administrator"/>
		<role name="Manager"/>
    	<role name="Owner" />			
    	<role name="Editor" />
	</permission>	
						
	<permission name="shuiwu.baoshui:View shuiwusummary" acquire="True">
		<role name="Site Administrator"/>
		<role name="Manager"/>
		<role name="ShuiguanyuanMember"/>		
	</permission>		
	<permission name="shuiwu.baoshui:View nashuiren" acquire="True">
		<role name="Site Administrator"/>
		<role name="Manager"/>
		<role name="ShuiguanyuanMember"/>		
	</permission>

  </permissions>		
</rolemap>